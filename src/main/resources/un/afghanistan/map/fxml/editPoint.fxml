<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="pane" maxHeight="300.0" maxWidth="300.0" prefHeight="300.0" prefWidth="300.0" style="-fx-background-color: #123456;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="un.afghanistan.map.controllers.EditPointController">
   <children>
      <VBox prefHeight="300.0" prefWidth="300.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="95.0" prefWidth="300.0">
               <children>
                  <Label text="Name: " textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="nameTextField">
                     <HBox.margin>
                        <Insets left="45.0" />
                     </HBox.margin>
                  </TextField>
               </children></HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Latitude" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="latitudeTextField">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Longitude" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin></Label>
                  <TextField fx:id="longitudeTextField">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin></TextField>
               </children></HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="95.0" prefWidth="300.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="40.0" text="File" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="fileTextField" prefHeight="26.0" prefWidth="124.0">
                     <HBox.margin>
                        <Insets left="45.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="browseBtn" mnemonicParsing="false" onAction="#browseButtonAction" prefHeight="22.0" prefWidth="24.0" text="...">
                     <HBox.margin>
                        <Insets bottom="2.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="167.0" prefHeight="95.0" prefWidth="300.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="150.0" text="Is in Kabul city SRM area" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
                  <CheckBox fx:id="isInKabulCheckbox" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="18.0" prefWidth="16.0" textAlignment="CENTER" textFill="WHITE" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#deleteButtonAction" text="Delete" textFill="RED" />
                  <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#saveButtonAction" prefWidth="60.0" text="Save">
                     <HBox.margin>
                        <Insets left="35.0" right="35.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelButtonAction" prefWidth="60.0" text="Cancel">
                     <HBox.margin>
                        <Insets right="30.0" />
                     </HBox.margin>
                  </Button>
               </children></HBox>
         </children>
      </VBox>
   </children>
</Pane>
